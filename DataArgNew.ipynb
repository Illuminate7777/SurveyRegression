   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import re\n",
    "\n",
    "def descriptive_statistics_custom(df, columns):\n",
    "    \"\"\"\n",
    "    For a given DataFrame and a list of columns, computes descriptive statistics:\n",
    "    mean, std, mad, min, 25%, 50% (median), 75%, and max.\n",
    "    Returns a DataFrame with one row per column.\n",
    "    \"\"\"\n",
    "    stats = {}\n",
    "    for col in columns:\n",
    "        # Convert column values to numeric and drop missing values\n",
    "        series = pd.to_numeric(df[col], errors='coerce').dropna()\n",
    "        # Skip if conversion leads to an empty series\n",
    "        if series.empty:\n",
    "            continue\n",
    "        stats[col] = {\n",
    "            \"mean\": series.mean(),\n",
    "            \"std\": series.std(),\n",
    "            \"mad\": np.mean(np.abs(series - series.mean())),\n",
    "            \"min\": series.min(),\n",
    "            \"25%\": series.quantile(0.25),\n",
    "            \"50%\": series.median(),\n",
    "            \"75%\": series.quantile(0.75),\n",
    "            \"max\": series.max()\n",
    "        }\n",
    "    return pd.DataFrame(stats).T\n",
    "\n",
    "def descriptive_stats_by_group(df, group_col, target_columns, output_prefix):\n",
    "    \"\"\"\n",
    "    Groups the DataFrame by group_col and computes descriptive statistics for target_columns.\n",
    "    Saves each group's statistics to a CSV file and prints the results.\n",
    "    Returns a dictionary with group names as keys and statistics DataFrames as values.\n",
    "    \"\"\"\n",
    "    groups = df[group_col].dropna().unique()\n",
    "    all_stats = {}\n",
    "    for grp in groups:\n",
    "        grp_df = df[df[group_col] == grp]\n",
    "        stats_df = descriptive_statistics_custom(grp_df, target_columns)\n",
    "        all_stats[grp] = stats_df\n",
    "        filename = f\"{output_prefix}_{group_col}_{grp}.csv\"\n",
    "        #stats_df.to_csv(filename)\n",
    "        print(f\"\\nDescriptive Statistics for {group_col} = {grp}:\")\n",
    "        print(stats_df)\n",
    "    return all_stats\n",
    "\n",
    "def main():\n",
    "    # Read the final combined dataset.\n",
    "    final_df = pd.read_csv(\"Final.csv\")\n",
    "    \n",
    "    # Rename columns if necessary (for example, if the header uses \"sample Persona\")\n",
    "    if \"sample Persona\" in final_df.columns:\n",
    "        final_df.rename(columns={\"sample Persona\": \"Persona\"}, inplace=True)\n",
    "    \n",
    "    # Remove extra spaces from column names\n",
    "    final_df.columns = final_df.columns.str.strip()\n",
    "    \n",
    "    #######################################\n",
    "    # Create Grouping Variables\n",
    "    #######################################\n",
    "    # 1. Religion_Group: Keep \"Muslim\" or \"Christian\"; otherwise \"Other\".\n",
    "    final_df[\"Religion_Group\"] = final_df[\"religion\"].apply(\n",
    "        lambda x: x if x in [\"Muslim\", \"Christian\"] else \"Other\"\n",
    "    )\n",
    "    \n",
    "    # 2. Country_Group: Keep \"US\" or \"Qatar\"; otherwise \"Other\".\n",
    "    final_df[\"Country_Group\"] = final_df[\"country\"].apply(\n",
    "        lambda x: x if x in [\"US\", \"Qatar\"] else \"Other\"\n",
    "    )\n",
    "    \n",
    "    # 3. Risk_Tolerance_Group: Use the values directly.\n",
    "    final_df[\"Risk_Tolerance_Group\"] = final_df[\"risk_tolerance\"]\n",
    "    \n",
    "    # 4. RelComm2_Group: Use the 'religion_freq_per_week' column.\n",
    "    final_df[\"RelComm2_Group\"] = final_df[\"religion_freq_per_week\"]\n",
    "    \n",
    "    #######################################\n",
    "    # Define Target Columns for Survey Descriptive Stats\n",
    "    #######################################\n",
    "    # Use survey questions Q1 to Q11.\n",
    "    survey_columns = [\n",
    "        \"Q1\", \"Q2\", \"Q3\", \"Q4\", \"Q5\", \n",
    "        \"Q6\", \"Q7\", \"Q8\", \"Q9\", \"Q10\", \"Q11\"\n",
    "    ]\n",
    "    \n",
    "    print(\"=== Descriptive Statistics for Survey Questions ===\")\n",
    "    print(\"\\nBy Religion Group:\")\n",
    "    descriptive_stats_by_group(final_df, \"Religion_Group\", survey_columns, \"survey_stats\")\n",
    "    \n",
    "    print(\"\\nBy Country Group:\")\n",
    "    descriptive_stats_by_group(final_df, \"Country_Group\", survey_columns, \"survey_stats\")\n",
    "    \n",
    "    print(\"\\nBy Risk Tolerance Group:\")\n",
    "    descriptive_stats_by_group(final_df, \"Risk_Tolerance_Group\", survey_columns, \"survey_stats\")\n",
    "    \n",
    "    print(\"\\nBy Religious Commitment 2 Group:\")\n",
    "    descriptive_stats_by_group(final_df, \"RelComm2_Group\", survey_columns, \"survey_stats\")\n",
    "    \n",
    "    #######################################\n",
    "    # ETF Descriptive Stats: Group columns by suffix (_q12, _q13, _q14, etc.)\n",
    "    #######################################\n",
    "    # Use a regular expression to extract the suffix number from each ETF allocation column name.\n",
    "    etf_groups = {}\n",
    "    for col in final_df.columns:\n",
    "        m = re.search(r'_q(\\d+)$', col)\n",
    "        if m:\n",
    "            suffix = m.group(1)\n",
    "            etf_groups.setdefault(suffix, []).append(col)\n",
    "    \n",
    "    # List of grouping variables to use for ETF descriptive stats.\n",
    "    grouping_vars = [\"Religion_Group\", \"Country_Group\", \"Risk_Tolerance_Group\", \"RelComm2_Group\"]\n",
    "    \n",
    "    # For each ETF group (by suffix), compute descriptive stats by each grouping variable.\n",
    "    for suffix, cols in etf_groups.items():\n",
    "        print(f\"\\n=== Descriptive Statistics for ETF Allocations (Suffix _q{suffix}) ===\")\n",
    "        for group_var in grouping_vars:\n",
    "            print(f\"\\nBy {group_var}:\")\n",
    "            descriptive_stats_by_group(final_df, group_var, cols, f\"etf_stats_{suffix}\")\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
    "\n"
   ]
