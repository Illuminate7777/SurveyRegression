    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "def descriptive_statistics_custom(df, columns):\n",
    "    \"\"\"\n",
    "    For a given DataFrame and a list of columns, computes descriptive statistics:\n",
    "    count, mean, std, MAD, min, 25th, 50th (median), 75th percentiles, and max.\n",
    "    Returns a DataFrame with one row per column.\n",
    "    \"\"\"\n",
    "    stats = {}\n",
    "    for col in columns:\n",
    "        # Convert column to numeric (ignoring non-numeric values)\n",
    "        series = pd.to_numeric(df[col], errors='coerce').dropna()\n",
    "        stats[col] = {\n",
    "            \"count\": series.count(),\n",
    "            \"mean\": series.mean(),\n",
    "            \"std\": series.std(),\n",
    "            \"mad\": np.mean(np.abs(series - series.mean())),\n",
    "            \"min\": series.min(),\n",
    "            \"25%\": series.quantile(0.25),\n",
    "            \"50%\": series.median(),\n",
    "            \"75%\": series.quantile(0.75),\n",
    "            \"max\": series.max()\n",
    "        }\n",
    "    return pd.DataFrame(stats).T\n",
    "\n",
    "def complete_descriptive_stats(file_path):\n",
    "    # Read the dataset from the provided file path\n",
    "    df = pd.read_csv(file_path)\n",
    "    print(f\"\\nProcessing file: {file_path}\")\n",
    "    \n",
    "    # --- Process Survey Data ---\n",
    "    # Define survey question columns based on the new names (Q1 to Q11)\n",
    "    survey_questions = [\"Q1\", \"Q2\", \"Q3\", \"Q4\", \"Q5\", \"Q6\", \"Q7\", \"Q8\", \"Q9\", \"Q10\", \"Q11\"]\n",
    "    # Filter only columns that exist in the DataFrame\n",
    "    existing_survey_columns = [col for col in survey_questions if col in df.columns]\n",
    "    if existing_survey_columns:\n",
    "        survey_stats = descriptive_statistics_custom(df, existing_survey_columns)\n",
    "        survey_output = f\"descriptive_statistics_survey_{file_path.split('.')[0]}.csv\"\n",
    "        survey_stats.to_csv(survey_output)\n",
    "        print(\"Descriptive Statistics for Survey Questions (Q1–Q11):\")\n",
    "        print(survey_stats)\n",
    "    else:\n",
    "        print(\"Survey question columns not found in the dataset.\")\n",
    "    \n",
    "    # --- Process ETF Data ---\n",
    "    # Identify ETF columns by checking for headers that contain an underscore and end with '_qXX'\n",
    "    etf_columns = [col for col in df.columns if '_' in col and col.split('_')[-1].startswith('q')]\n",
    "    \n",
    "    # Group ETF columns based on the suffix (i.e., the question/group number)\n",
    "    etf_groups = {}\n",
    "    for col in etf_columns:\n",
    "        # Extract the suffix (e.g., \"q12\")\n",
    "        suffix = col.split('_')[-1]\n",
    "        # Remove the 'q' so that group 'q12' becomes '12'\n",
    "        group = suffix[1:] if suffix.startswith('q') else suffix\n",
    "        etf_groups.setdefault(group, []).append(col)\n",
    "    \n",
    "    # Compute descriptive statistics for each ETF group and output to separate CSV files\n",
    "    for grp, cols in etf_groups.items():\n",
    "        stats_df = descriptive_statistics_custom(df, cols)\n",
    "        etf_output = f\"descriptive_statistics_etf_group_{grp}_{file_path.split('.')[0]}.csv\"\n",
    "        stats_df.to_csv(etf_output)\n",
    "        print(f\"\\nDescriptive Statistics for ETF Allocations in Group {grp}:\")\n",
    "        print(stats_df)\n",
    "\n",
    "def main():\n",
    "    # List of files to process\n",
    "    files = [\"percent_AR_deepseekLLama.csv\", \n",
    "        \"percent_AR_deepseekR1.csv\", \n",
    "        \"percent_AR_deepseekV3.csv\", \n",
    "        \"percent_AR_gpt4ominioutput.csv\", \n",
    "        \"percent_AR_grok-2-1212.csv\", \n",
    "        \"percent_AR_LLamaNm.csv\", \n",
    "        \"Final.csv\"]  # Add additional file names as needed\n",
    "    \n",
    "    for file in files:\n",
    "        print(f\"\\nComputing descriptive statistics for {file}...\")\n",
    "        complete_descriptive_stats(file)\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()\n"
   ]
