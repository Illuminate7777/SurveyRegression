    "import pandas as pd\n",
    "import numpy as np\n",
    "import statsmodels.api as sm\n",
    "\n",
    "def perform_multinomial_regression(df):\n",
    "    \"\"\"\n",
    "    Prepares the data and performs a multinomial logistic regression\n",
    "    where the dependent variable is the 'attitude' determined from\n",
    "    haram, neutral, and halal sum scores. The independent variables include:\n",
    "      - Age (raw numeric)\n",
    "      - risk, islamic_factors, societal (group averages)\n",
    "      - Categorical variables that are dummy-coded:\n",
    "            religion_freq_per_week (as religion_freq_group),\n",
    "            education_level,\n",
    "            education_field,\n",
    "            investment_focus,\n",
    "            experience.\n",
    "    Returns the fitted MNLogit model.\n",
    "    \"\"\"\n",
    "    # --- Data preparation ---\n",
    "    # Convert percentage columns to numeric\n",
    "    pct_cols = [\n",
    "        'seg_amount_q12', 'agt_amount_q12', 'cce_amount_q12',\n",
    "        'gmb_amount_q12', 'bcd_amount_q12', 'emv_amount_q12',\n",
    "        'dst_amount_q12', 'cab_amount_q12', 'bbg_amount_q12'\n",
    "    ]\n",
    "    for col in pct_cols:\n",
    "        df[col] = pd.to_numeric(df[col], errors='coerce')\n",
    "    \n",
    "    # Convert age to numeric (using raw age)\n",
    "    df['Age'] = pd.to_numeric(df['age'], errors='coerce')\n",
    "    \n",
    "    # Process the religion frequency as a categorical variable.\n",
    "    df['religion_freq_group'] = df['religion_freq_per_week'].astype(str).str.strip()\n",
    "    \n",
    "    # One-hot encode categorical variables:\n",
    "    # religion_freq_group, education_level, education_field, investment_focus, and experience.\n",
    "    df = pd.get_dummies(df, \n",
    "                        columns=['religion_freq_group', 'education_level', 'education_field', \n",
    "                                 'investment_focus', 'experience'], \n",
    "                        drop_first=True)\n",
    "    \n",
    "    # --- Compute attitude from sum scores ---\n",
    "    df['haram_sum']   = df['seg_amount_q12'] + df['agt_amount_q12'] + df['cce_amount_q12']\n",
    "    df['neutral_sum'] = df['gmb_amount_q12'] + df['bcd_amount_q12'] + df['emv_amount_q12']\n",
    "    df['halal_sum']   = df['dst_amount_q12'] + df['cab_amount_q12'] + df['bbg_amount_q12']\n",
    "    \n",
    "    def determine_attitude(row):\n",
    "        sums = {\n",
    "            'Haram_12': row['haram_sum'],\n",
    "            'Neutral_12': row['neutral_sum'],\n",
    "            'Halal_12': row['halal_sum']\n",
    "        }\n",
    "        return max(sums, key=sums.get)\n",
    "    \n",
    "    df['attitude'] = df.apply(determine_attitude, axis=1)\n",
    "    # Create a categorical version of the attitude\n",
    "    df['attitude_cat'] = pd.Categorical(df['attitude'], categories=['Haram_12', 'Neutral_12', 'Halal_12'])\n",
    "    \n",
    "    # --- Create additional numeric predictors ---\n",
    "    df['risk'] = df[['Q1', 'Q11']].mean(axis=1)\n",
    "    df['islamic_factors'] = df[['Q4', 'Q6', 'Q9', 'Q10', 'Q11']].mean(axis=1)\n",
    "    df['societal'] = df[['Q7', 'Q8']].mean(axis=1)\n",
    "    \n",
    "    # --- Build the feature set ---\n",
    "    # Base features: Age, risk, islamic_factors, and societal.\n",
    "    base_features = ['Age', 'risk', 'islamic_factors', 'societal']\n",
    "    # Identify all dummy variables generated for our categorical predictors.\n",
    "    dummy_features = [col for col in df.columns \n",
    "                      if (col.startswith('religion_freq_group_') or \n",
    "                          col.startswith('education_level_') or\n",
    "                          col.startswith('education_field_') or\n",
    "                          col.startswith('investment_focus_') or\n",
    "                          col.startswith('experience_'))]\n",
    "    features = base_features + dummy_features\n",
    "    \n",
    "    X = df[features]\n",
    "    # Map the attitude categorical variable to numeric codes:\n",
    "    # 'Haram_12' -> 0, 'Neutral_12' -> 1, 'Halal_12' -> 2.\n",
    "    y = df['attitude_cat'].cat.codes\n",
    "    \n",
    "    # Combine X and y and drop rows with any missing values.\n",
    "    model_df = pd.concat([X, y.rename('attitude_code')], axis=1).dropna()\n",
    "    X = model_df[features]\n",
    "    y = model_df['attitude_code']\n",
    "    \n",
    "    # Ensure all predictor columns are numeric.\n",
    "    X = X.astype(float)\n",
    "    \n",
    "    # --- Prepare for statsmodels ---\n",
    "    # Add a constant term to the predictors.\n",
    "    X = sm.add_constant(X, has_constant='add')\n",
    "    \n",
    "    # --- Fit the multinomial logistic regression model ---\n",
    "    model = sm.MNLogit(y, X).fit(method='newton', maxiter=100, disp=False)\n",
    "    return model\n",
    "\n",
    "def main():\n",
    "    # Read the final CSV dataset.\n",
    "    df = pd.read_csv(\"Final.csv\")\n",
    "    # Clean column names if necessary.\n",
    "    df.columns = df.columns.str.strip()\n",
    "    \n",
    "    # (Optional) Further process the dataset here.\n",
    "    df['country'] = df['country'].apply(lambda x: x if x in ['US', 'Qatar'] else 'Other')\n",
    "    \n",
    "    # Run the multinomial logistic regression.\n",
    "    model = perform_multinomial_regression(df)\n",
    "    \n",
    "    # Print a nicely formatted summary.\n",
    "    print(model.summary())\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
    "\n"
   ]
