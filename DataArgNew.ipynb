    "import pandas as pd\n",
    "import statsmodels.api as sm\n",
    "\n",
    "def perform_regression_for_question_all(df, question_col):\n",
    "    \"\"\"\n",
    "    Performs an OLS regression where:\n",
    "      - The dependent variable is the given question (e.g., \"Question 1\")\n",
    "      - The independent variables are every other column (except identifiers such as \"Persona\")\n",
    "        and any column that contains \"_pct_\" in its name.\n",
    "    \n",
    "    Non-numeric predictors are automatically dummy-coded.\n",
    "    Rows with missing values in either the DV or any IV are dropped.\n",
    "    \n",
    "    Returns the fitted model (or None if insufficient data).\n",
    "    \"\"\"\n",
    "    # Define any identifier columns to exclude (e.g., \"Persona\")\n",
    "    identifier_cols = {\"Persona\"}\n",
    "    \n",
    "    # Exclude any column with \"_pct_\" in its name, in addition to the DV and identifiers.\n",
    "    independent_vars = [\n",
    "        col for col in df.columns \n",
    "        if col not in identifier_cols \n",
    "        and col != question_col \n",
    "        and \"_amount_\" not in col\n",
    "    ]\n",
    "    \n",
    "    # Subset the data to the DV and predictors.\n",
    "    sub_df = df[[question_col] + independent_vars].copy()\n",
    "    \n",
    "    # Convert the dependent variable to numeric.\n",
    "    sub_df[question_col] = pd.to_numeric(sub_df[question_col], errors='coerce')\n",
    "    \n",
    "    # Drop rows with missing values in the DV and predictors.\n",
    "    sub_df = sub_df.dropna(subset=[question_col] + independent_vars)\n",
    "    if sub_df.empty:\n",
    "        print(f\"No valid data for regression on {question_col}\")\n",
    "        return None\n",
    "    \n",
    "    # Dummy-code categorical predictors.\n",
    "    X = pd.get_dummies(sub_df[independent_vars], drop_first=False)\n",
    "    X = X.astype(float)\n",
    "    \n",
    "    # Dependent variable.\n",
    "    y = sub_df[question_col]\n",
    "    \n",
    "    # Fit OLS (no constant added so that all dummy categories are kept).\n",
    "    model = sm.OLS(y, X).fit()\n",
    "    return model\n",
    "\n",
    "def run_regressions_for_questions(df):\n",
    "    \"\"\"\n",
    "    Loops over \"Question 1\" to \"Question 11\" and performs regressions\n",
    "    with each as the dependent variable and all remaining columns (except \"Persona\"\n",
    "    and any column containing \"_pct_\") as predictors.\n",
    "    Prints the summary for each regression.\n",
    "    \n",
    "    Returns a dictionary of fitted models keyed by question.\n",
    "    \"\"\"\n",
    "    regression_results = {}\n",
    "    for i in range(1, 12):\n",
    "        q_col = f\"Q{i}\"\n",
    "        print(f\"\\n=== Running regression for {q_col} ===\")\n",
    "        model = perform_regression_for_question_all(df, q_col)\n",
    "        if model is not None:\n",
    "            regression_results[q_col] = model\n",
    "            print(model.summary())\n",
    "        else:\n",
    "            print(f\"Regression for {q_col} could not be run due to insufficient data.\")\n",
    "    return regression_results\n",
    "\n",
    "def main():\n",
    "    # Read the final combined dataset.\n",
    "    df = pd.read_csv(\"Final.csv\")\n",
    "    \n",
    "    # Clean up column names (remove extra spaces, etc.)\n",
    "    df.columns = df.columns.str.strip()\n",
    "    \n",
    "    # Run regressions for questions Q1–Q11.\n",
    "    results = run_regressions_for_questions(df)\n",
    "    \n",
    "    # (Optional) Save or further process the results.\n",
    "    \n",
    "if __name__ == \"__main__\":\n",
    "    main()\n",
    "\n"
   ]
