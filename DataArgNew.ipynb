   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import glob\n",
    "import random\n",
    "\n",
    "# -------------- 1) Dot-cleaning function --------------\n",
    "def fix_dots(val):\n",
    "    \"\"\"\n",
    "    Convert 'val' to string, remove trailing dots, \n",
    "    and remove extra dots (keep only the first).\n",
    "    \"\"\"\n",
    "    s = str(val).strip()\n",
    "    # Remove trailing dots\n",
    "    while s.endswith('.'):\n",
    "        s = s[:-1]\n",
    "    # If multiple dots exist, keep only the first\n",
    "    dot_index = s.find('.')\n",
    "    if dot_index != -1:\n",
    "        second_dot = s.find('.', dot_index + 1)\n",
    "        while second_dot != -1:\n",
    "            s = s[:second_dot] + s[second_dot+1:]\n",
    "            second_dot = s.find('.', dot_index + 1)\n",
    "    return s\n",
    "\n",
    "# -------------- 2) Rounding logic for ETF percentages --------------\n",
    "def adjust_etf_percentages(row, columns):\n",
    "    \"\"\"\n",
    "    Given a row of raw ETF amounts for the given columns:\n",
    "      1) Clean the values using fix_dots.\n",
    "      2) Convert to numeric, treating errors as 0.\n",
    "      3) Calculate raw percentages.\n",
    "      4) Floor each percentage to the nearest multiple of 5.\n",
    "      5) Distribute any leftover in increments of 5% to the highest raw percentages.\n",
    "    Returns a Series of ETF percentages (as integers) that sum to 100.\n",
    "    \"\"\"\n",
    "    cleaned = row[columns].apply(fix_dots)\n",
    "    values = pd.to_numeric(cleaned, errors='coerce').fillna(0)\n",
    "    total = values.sum()\n",
    "    if total == 0:\n",
    "        return pd.Series([0] * len(columns), index=columns)\n",
    "    \n",
    "    raw_pct = (values / total) * 100\n",
    "    floor_vals = np.floor(raw_pct / 5) * 5\n",
    "    floor_vals = floor_vals.astype(int)\n",
    "    \n",
    "    total_floor = floor_vals.sum()\n",
    "    leftover = 100 - total_floor\n",
    "    increments = leftover // 5\n",
    "\n",
    "    # Distribute the leftover increments to the columns with highest raw percentages\n",
    "    indices = np.argsort(-raw_pct)  # descending order of raw_pct\n",
    "    i = 0\n",
    "    while increments > 0:\n",
    "        floor_vals[indices[i]] += 5\n",
    "        increments -= 1\n",
    "        i += 1\n",
    "        if i >= len(floor_vals):\n",
    "            i = 0\n",
    "    return pd.Series(floor_vals, index=columns)\n",
    "\n",
    "# -------------- 3) Load CSVs --------------\n",
    "file_paths = glob.glob(\"*.csv\")\n",
    "dfs = [pd.read_csv(fp, encoding=\"latin-1\") for fp in file_paths]\n",
    "\n",
    "if len(dfs) != 6:\n",
    "    print(f\"Warning: Expected 6 CSV files, but found {len(dfs)}.\")\n",
    "\n",
    "# We will work on questions Q12, Q13, Q14 for the ETF columns later.\n",
    "question_numbers = [12, 13, 14]\n",
    "\n",
    "# -------------- 4) Build the Final Combined DataFrame --------------\n",
    "# Non-question columns that remain unchanged\n",
    "non_q_columns = [\n",
    "    'age', 'gender', 'country', 'education_level',\n",
    "    'education_field', 'investment_focus', 'experience',\n",
    "    'risk_tolerance', 'religion', 'religion_freq_per_week'\n",
    "]\n",
    "\n",
    "# Start the final result with non-question columns from the first CSV.\n",
    "result_df = dfs[0][non_q_columns].copy()\n",
    "\n",
    "# ---- Q15: Randomly pick a non-null value from available CSVs ----\n",
    "q15_data = pd.concat([df['Q15'] for df in dfs if 'Q15' in df.columns], axis=1)\n",
    "result_df['Q15'] = q15_data.apply(\n",
    "    lambda row: random.choice(row.dropna().tolist()) if not row.dropna().empty else np.nan,\n",
    "    axis=1\n",
    ")\n",
    "\n",
    "# ---- Q1 to Q11: Average them across CSVs ----\n",
    "for q in range(1, 12):\n",
    "    col_name = f\"Q{q}\"\n",
    "    series_list = []\n",
    "    for df in dfs:\n",
    "        if col_name in df.columns:\n",
    "            series_list.append(df[col_name])\n",
    "    if series_list:\n",
    "        col_data = pd.concat(series_list, axis=1)\n",
    "        result_df[col_name] = col_data.mean(axis=1, skipna=True)\n",
    "\n",
    "# ---- Q12 to Q14: Process ETF columns from underlying raw amounts ----\n",
    "# Instead of averaging percentages (which can lead to sums != 100),\n",
    "# we average the raw amounts first and then compute ETF percentages.\n",
    "for q in question_numbers:\n",
    "    q_cols = [\n",
    "        f\"seg_amount_q{q}\", f\"agt_amount_q{q}\", f\"cce_amount_q{q}\",\n",
    "        f\"gmb_amount_q{q}\", f\"bcd_amount_q{q}\", f\"emv_amount_q{q}\",\n",
    "        f\"dst_amount_q{q}\", f\"cab_amount_q{q}\", f\"bbg_amount_q{q}\"\n",
    "    ]\n",
    "    \n",
    "    # Build a DataFrame of averaged raw amounts for the ETF columns.\n",
    "    avg_df = pd.DataFrame(index=dfs[0].index)\n",
    "    for col in q_cols:\n",
    "        col_series_list = []\n",
    "        for df in dfs:\n",
    "            if col in df.columns:\n",
    "                # Convert to string, clean and then to numeric\n",
    "                col_series = pd.to_numeric(df[col].apply(fix_dots), errors='coerce').fillna(0)\n",
    "                col_series_list.append(col_series)\n",
    "        if col_series_list:\n",
    "            avg_df[col] = pd.concat(col_series_list, axis=1).mean(axis=1, skipna=True)\n",
    "        else:\n",
    "            avg_df[col] = np.nan\n",
    "\n",
    "    # For each row, compute ETF percentages using the rounding logic.\n",
    "    pct_df = avg_df.apply(lambda row: adjust_etf_percentages(row, q_cols), axis=1)\n",
    "    \n",
    "    # Add the computed ETF percentages to the result DataFrame.\n",
    "    for col in q_cols:\n",
    "        result_df[col] = pct_df[col]\n",
    "\n",
    "# -------------- 5) Save the Final CSV --------------\n",
    "result_df.to_csv(\"averaged_results.csv\", index=False)\n",
    "print(\"Averaging complete. Results saved to averaged_results.csv\")\n",
    "\n"
   ]
